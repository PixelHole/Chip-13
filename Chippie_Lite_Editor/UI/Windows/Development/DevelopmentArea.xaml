<UserControl x:Class="Chippie_Lite_WPF.UI.Windows.DevelopmentArea"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:development="clr-namespace:Chippie_Lite_WPF.UI.Windows.Development"
             xmlns:elements="clr-namespace:Chippie_Lite_WPF.UI.Elements"
             mc:Ignorable="d"
             Background="Transparent"
             SnapsToDevicePixels="True"
             d:DesignHeight="300" d:DesignWidth="400">
    
    <Grid Background="{StaticResource 'Dark Blue'}">
        <development:CodeEditView x:Name="CodeEditControl" Margin="0"/>
        <development:MemoryEditView x:Name="MemoryEditControl" Visibility="Collapsed"/>
        <development:RunView x:Name="RunWindow" Visibility="Collapsed"/>
    </Grid>
</UserControl>
